{
  "publisher": "arthurdanjou",
  "name": "theme-artlab",
  "displayName": "ArtLab Theme",
  "version": "1.0.2-beta.1",
  "packageManager": "bun@1.3.8",
  "description": "ArtLab theme for VS Code",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arthurdanjou/vscode-theme-artlab"
  },
  "bugs": {
    "url": "https://github.com/arthurdanjou/vscode-theme-artlab/issues"
  },
  "keywords": [
    "theme",
    "artlab",
    "light",
    "dark"
  ],
  "categories": [
    "Themes"
  ],
  "exports": {
    "./*": "./*"
  },
  "icon": "icon.png",
  "files": [
    "LICENSE.md",
    "extra/*",
    "icon.png",
    "themes/*"
  ],
  "engines": {
    "vscode": "1.109.3"
  },
  "contributes": {
    "themes": [
      {
        "label": "ArtLab Light",
        "uiTheme": "vs",
        "path": "./themes/artlab-light.json"
      },
      {
        "label": "ArtLab Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/artlab-dark.json"
      }
    ]
  },
  "scripts": {
    "build": "tsx scripts/index.ts",
    "dev": "tsx watch scripts/index.ts",
    "vscode:prepublish": "npm run build",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp --all",
    "package": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "7.4.3",
    "@antfu/utils": "9.3.0",
    "@types/color": "4.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "25.2.3",
    "@vscode/vsce": "^3.7.1",
    "bumpp": "^10.2.3",
    "color": "^5.0.0",
    "eslint": "10.0.0",
    "fs-extra": "^11.3.1",
    "ovsx": "^0.10.5",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vsxpub": "^0.1.0"
  },
  "preview": true
}
